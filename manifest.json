{
  "manifest_version": 2,
  "name": "宗明藥局蝦皮小幫手",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://seller.shopee.tw/*",
        "https://external2.shopee.tw/*",
        "file:///*"
      ],
      "js": [
        "jquery-3.7.1.min.js",
        "printShopeeOrder.js",
        "process7ElevenTable.js",
        "processFamilyMartTable.js",
        "getProductCode.js",
        "content.js"
      ],
      "css": [
        "font.css"
      ],
      "run_at": "document_end"
    }
  ]
}